msgstr "Senpara '<' kaj '>' en tipara modifilo. Erara mesaĝo: %s"

#: ../../lib/Locale/Po4a/Man.pm:1123
#, perl-format
msgid "Unknown '<' or '>' sequence. Faulty message: %s"
