#: lib/Locale/Po4a/Man.pm:108
msgid ""
"L<Locale::Po4a::Pod(3pm)>, L<Locale::Po4a::TransTractor(3pm)>, L<po4a(7)|"
"po4a.7>"
msgstr ""
"L<Locale::Po4a::Pod(3pm)>, L<Locale::Po4a::TransTractor(3pm)>, L<po4a(7)|"
"po4a.7>"

